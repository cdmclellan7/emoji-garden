{
  "name": "emoji-garden",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "node ./bin/www.js",
    "test": "node -r dotenv/config --experimental-vm-modules node_modules/jest/bin/jest.js",
    "dev": "nodemon -r dotenv/config ./bin/www.js",
    "db-create-emojis-table": "node -r dotenv/config ./db/scripts/createEmojisTable.js",
    "db-reset-emojis-table": "node -r dotenv/config ./db/scripts/resetEmojisTable.js",
    "db-create-users-table": "node -r dotenv/config ./db/scripts/createUsersTable.js",
    "db-reset-users-table": "node -r dotenv/config ./db/scripts/resetUsersTable.js",
    "db-create-gardens-table": "node -r dotenv/config ./db/scripts/createGardensTable.js",
    "db-reset-gardens-table": "node -r dotenv/config ./db/scripts/resetGardensTable.js",
    "db-create-all-tables": "npm run db-create-users-table && npm run db-create-gardens-table && npm run db-create-emojis-table"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "connect-flash": "^0.1.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "ejs": "^3.1.6",
    "express": "~4.16.1",
    "express-session": "^1.17.2",
    "morgan": "~1.9.1",
    "passport": "^0.5.2",
    "passport-local": "^1.0.0",
    "pg": "^8.7.1"
  },
  "devDependencies": {
    "dotenv": "^10.0.0",
    "jest": "^27.4.5",
    "nodemon": "^2.0.15",
    "supertest": "^6.1.6"
  }
}
